<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="HTTP_Client.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:HTTP_Client.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="HTTP_Client">

    <ShellContent
        Title="Listar Postagens"
        ContentTemplate="{DataTemplate local:PostView}"
        Route="PostView" />
    <ShellContent
    Title="CriarPostagens"
    ContentTemplate="{DataTemplate local:PostView}"
    Route="CreatePostView" />    
        
    
    
</Shell>
